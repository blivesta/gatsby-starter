{
  "name": "gatsby-starter",
  "description": "Gatsby starter for creating a websites using React.js",
  "version": "0.0.0",
  "main": "n/a",
  "scripts": {
    "build": "npm run clean && npm run svg && gatsby build",
    "clean": "rm -rf ./svgpack && rm -rf ./public",
    "deploy": "npm run clean && npm run svg && gatsby build --prefix-links && gh-pages -d public",
    "dev": "npm run clean && npm run svg && gatsby develop",
    "lint": "standard",
    "svg": "svgpack ./svg/*.svg -d ./svgpack -s ./svgpack-react-svgsprite-template.svg",
    "test": "npm run lint"
  },
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "gatsby": "^0.12.5",
    "history": "v2.x",
    "lodash": "^4.15.0",
    "moment": "^2.14.1",
    "normalize.css": "^4.2.0",
    "react": "^15.3.1",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.3.1",
    "react-router": "^2.6.1",
    "react-google-analytics": "^0.2.0",
    "superagent": "^2.2.0",
    "safe-access": "^0.1.0",
    "underscore.string": "^3.3.4",
    "sircus": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-preset-react-hmre": "^1.1.1",
    "gh-pages": "^0.11.0",
    "image-webpack-loader": "^2.0.0",
    "postcss-calc": "^5.3.0",
    "postcss-color-function": "^2.0.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-import": "^8.1.2",
    "postcss-reporter": "^1.4.1",
    "standard": "^7.1.2",
    "svgpack": "^2.1.0"
  },
  "author": "blivesta",
  "homepage": "http://git.blivesta.com/gatsby-starter",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blivesta/gatsby-starter.git"
  }
}
